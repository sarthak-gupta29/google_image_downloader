{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PEXELS_API_KEY": "@pexels-api-key"
  },
  "engines": {
    "python": "3.12.3"
  },
  "installCommand": "pip install -r requirements.txt"
}
